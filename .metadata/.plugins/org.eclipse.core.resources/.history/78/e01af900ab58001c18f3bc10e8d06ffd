	window.onload = function()
	{
		var temp = "printString('First')";

		/* JSON */
		var printList =
		{
			"1" : "printString('Hi JSON~!!!')",
			"2" : temp,
			"3" : "alert('Second')",
			"4" : "printString('Third')"
		}

		for (var i = 1; i < 5; i++) // 1 2 3 4 
		{
			eval(printList[i]);
		}

	}

